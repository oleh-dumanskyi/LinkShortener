@{
    Layout = "/Views/_Layout.cshtml";
}
<p><p><a asp-controller="Url" asp-action="GetAll" class="link">Назад</a></p></p>

<table style="border-collapse: collapse;
              border-color: lightgrey;
              border-style: solid;">
    <tr>
        <th style="padding:0 15px 0 15px;">Країна</th>
        <th style="padding:0 15px 0 15px;">Кількість переходів</th>
    </tr>
    @foreach (var element in ViewBag.Statistics)
    {
        <tr>
            <td style="padding:0 15px 0 15px;">@element.Key</td>
            <td style="padding:0 15px 0 15px;">@element.Value</td>
        </tr>
    }
</table>